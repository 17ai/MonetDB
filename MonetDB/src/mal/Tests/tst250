# The series tst2?? is a patched version of suite_00
# The first and foremost BAT operations

b:= new(int,int);
err := insert(b,1,15);
err := insert(b,2,4);
n0  := int(nil);
err := insert(b,3,n0);
err := insert(b,4,16);
err := insert(b,5,25);
err := insert(b,6,36);
err := insert(b,7,49);
err := insert(b,8,64);
err := insert(b,9,n0);
err := insert(b,n0,n0);
err := insert(b,n0,81);
print(b);
i := -1:int;
barrier v:= batloop(i,b);
	print(i);
redo v;
end v;
