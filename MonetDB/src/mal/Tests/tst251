# The series tst2?? is a patched version of suite_00
# The first and foremost BAT operations

b:= new(int,int);
insert(b,1,15);
insert(b,2,4);
n0  := int(nil);
insert(b,3,n0);
insert(b,4,16);
insert(b,5,25);
insert(b,6,36);
insert(b,7,49);
insert(b,8,64);
insert(b,9,n0);
insert(b,n0,n0);
insert(b,n0,81);
print(b);
i := -1:int;
h:= 0:int;
t:= 0:int;
barrier v:= batloop(i,b,h,t);
	printf("[%d,",i);
	h:= getHead(i,b);
	printf("%d,",h);
	t:= getTail(i,b);
	printf("%d]\n",t);
redo v;
end v;


# String stuff
c:= new(int,str);
insert(c,1,"hello");
insert(c,2,"brave");
insert(c,3,"world");
insert(c,4,"");
x:= str(nil);
insert(c,5,x);
insert(c,6,"!");
print(c);
i := -1:int;
ts:= "";
barrier v:= batloop(i,c,h,ts);
	printf("[%d,",i);
	printf("%d,",h);
	printf("%s]\n",ts);
	redo v;
end v;

