# partial definition of the object space for MIL
# it is subordinate to the MIL space dealing with persistent bats
# CONST STORE_MEM   := 0; 
# CONST STORE_MMAP  := 1; 
# CONST STORE_COMPR := 2; 

space mil_constants(mil_bats);
	STORE_MEM   := 0;
	STORE_MMAP  := 1;
	STORE_COMPR := 2;

# binding uses the 'root' versions.
function bind(v:str):any;
	w:= lookup(v);
	return w:
end function;

# release is overloaded for testing only
function release(v:any):void;
	printf("releasing an object:");
	printft(v);
	root.release(v);	# pass on
end function;

