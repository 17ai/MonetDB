# A few system calls
err:= print("hello world");
err:= print(0);
err:= print(1:lng);
err:= print('c');
err:= print(true);
pi := 3.147;
err:= print(pi);
err:= printf("hello\n");
err:= printf("%5d",0);
err:= printf("%5d",1:lng);
err:= printf("%c",'c');
err:= printf("%s", true);
err:= printf("%6.2f",pi);
err:= printf("single line %s \n","hello");
